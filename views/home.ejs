
<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <title>Integriy Academy</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css">
    <link href="/css/stylesheet.css" rel="stylesheet" type="text/css">
    <style>

    </style>
</head>

<body>
    <div class="container">
        <%- include('partials/navbar') %>
        <div class="jumbotron">
            <h1>Integrify Student's Gallery</h1>
            <p class="lead">Let's come and check everyone</p>
        </div>

        <div class="row">
            <div class="col-lg-12">
              <div class="list-group">
                <!-- loop over blog posts and render them -->
                <% data.forEach((item) => { %>
                   <div class="list-group-items">
                    <a href="/studentDetails/<%= item.firstName %>" ><img src="/images/<%= item.src %>" class="images"/></a>
                     <div class="list-group-content">
                      <h4 class="list-group-item-heading"> Name :<%= item.firstName %> <%= item.lastName %></h4>
                      <p class="list-group-item-text">Title: <%= item.title %></p>
                    </div>
                    </div>

                <% }) %>
            </div>
            </div>
        </div>
        <%- include('partials/footer') %>
    </div>
</body>

</html>
